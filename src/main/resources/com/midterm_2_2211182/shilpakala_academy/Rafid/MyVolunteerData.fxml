<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="670.0" style="-fx-background-color: #9ACD32;" stylesheets="@chart-style.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oopproject2024.MyVolunteerDataController">
    <children>
        <BarChart fx:id="barChart" layoutX="261.0" layoutY="121.0" prefHeight="381.0" prefWidth="396.0">
            <xAxis>
                <CategoryAxis side="BOTTOM" fx:id="xAxis" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="yAxis" side="LEFT" />
            </yAxis>
        </BarChart>
        <Button fx:id="goBackBtnFxID" layoutX="33.0" layoutY="29.0" mnemonicParsing="false" onAction="#goBackBtnOnClick" style="-fx-background-color: white;" text="Go Back" textFill="#bf0808">
            <font>
                <Font name="System Bold" size="13.0" />
            </font></Button>
        <Label layoutX="210.0" layoutY="24.0" text="My Volunteering Data" textFill="WHITE">
            <font>
                <Font name="System Bold Italic" size="24.0" />
            </font>
        </Label>
        <TableView fx:id="completedEventsTableView" layoutX="33.0" layoutY="138.0" prefHeight="339.0" prefWidth="215.0">
            <columns>
                <TableColumn fx:id="eventNameColumn" prefWidth="214.0" text="Completed Events" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
